<idea-plugin>
    <id>org.example.test-plugin</id>
    <name>Fast Code</name>
    <vendor email="929371489@qq.com" url="http://www.cnqisoft.com">cnqisoft</vendor>

    <description><![CDATA[
    根据配置文件快速生成Java代码和数据库.<br>
    <em>Java SpringMVC MyBatis</em>
    ]]></description>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <!-- Add your actions here -->
        <action id="GenerateCodeAction" class="com.cnqisoft.fastcode.GenerateCodeAction" text="生成代码"
                description="生成代码">
            <add-to-group group-id="CodeMenu" anchor="first"/>
        </action>
    </actions>
</idea-plugin>