{
  "name": "model",
  "fields": [
    {
      "field": "modelName",
      "type": "String",
      "required": true
    },
    {
      "field": "fileType",
      "type": "String",
      "input": false,
      "enum": [
        {
          "name": "singleModel",
          "value": "单模型"
        },
        {
          "name": "integrateModel",
          "value": "集成模型"
        }
      ]
    },

    {
      "field": "translateStatus",
      "type": "String",
      "input": false,
      "enum": [
        {
          "name": "prepare",
          "value": "等待转换"
        },
        {
          "name": "success",
          "value": "转换成功"
        },
        {
          "name": "processing",
          "value": "转换中"
        },
        {
          "name": "failed",
          "value": "转换失败"
        }
      ]
    },
    {
      "field": "sync",
      "type": "Number",
      "input": false,
      "enum": [
        {
          "name": "notSynchronized",
          "value": 0
        },
        {
          "name": "synchronized",
          "value": 1
        },
        {
          "name": "synchronizing",
          "value": 2
        }
      ]
    },
    {
      "field": "modelFilePath",
      "type": "String",
      "required": true
    },
    {
      "field": "fileId",
      "type": "Long",
      "required": true,
      "unique": true
    }
  ],
  "files": [
    "modelFile"
  ]
}